{
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pydocstyleEnabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pycodestyleArgs": ["--max-line-length=200"],
  "python.linting.pylintArgs": [
    "--disable=F0401,C0103",
    "--extension-pkg-whitelist=cv2,torch",
    "--generated-members=numpy.* ,torch.*, airflow.*"
  ],
  "python.linting.pydocstyleArgs": ["--ignore=D105,D107,D203,D213"],
  "python.formatting.provider": "black",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.rulers": [79, 100],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jupyter.alwaysTrustNotebooks": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.defaultInterpreterPath": "C:\\Users\\Andreas Chandra\\AppData\\Local\\Continuum\\anaconda3\\python.exe",
  "diffEditor.ignoreTrimWhitespace": false
}
